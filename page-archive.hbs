{{!< default}}

{{!-- The main content area --}}
<main id="site-main" class="site-main outer">
    <div class="inner">
        {{#get "tags" limit="all" filter="name:-[business, campus-life, news-in-pictures, off-campus, opinion, politics, science-and-technology, sports]+tag:-arts"}}
                <ul class="tags">
                    {{#foreach tags}}
                        <li>
                            <a href="{{url}}">{{name}}</a>
                        </li>
                    {{/foreach}}
                </ul>
        {{/get}}
    </div>
</main>
